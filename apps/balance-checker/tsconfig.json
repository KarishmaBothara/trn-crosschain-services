{
  "extends": "../../tsconfig.base.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
  }
}
